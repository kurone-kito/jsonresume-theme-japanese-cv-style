style(type='text/css')
  | @page { margin: 2.5rem; }
  | body { line-height: 100% }
  | p, ul { margin-bottom: 0.5rem; }
  | section.page { break-before: page; }
  | .card, tr { break-inside: avoid-page; }
  | .card { margin-bottom: 0.2rem; }
  | .card-body { padding: 0.5rem; }
  | .h6 { margin-bottom: 0.1rem; }
